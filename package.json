    "main": "index.js",
    "scripts": {
        "dev:server": "npx nodemon --exec 'node src/server.js' --watch src/server.js --watch src/server -e js",
        "dev:client": "npx nodemon --exec 'node src/client.js' --watch src/client.js -e js",
        "start:server": "node src/server.js",
        "start:client": "node src/client.js",
        "up:server": "docker-compose -f docker/server.yml up -d",
        "down:server": "docker-compose -f docker/server.yml down",
        "up:client": "docker-compose -f docker/client.yml up -d",
        "down:client": "docker-compose -f docker/client.yml down"
